#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d", &N);
    int arr[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    int largest = arr[0];
    int secondLargest = -10001; 
    int found = 0;
    for (int i = 1; i < N; i++) {
        if (arr[i] > largest) {
            largest = arr[i];
        }
    }
    for (int i = 0; i < N; i++) {
        if (arr[i] != largest && arr[i] > secondLargest) {
            secondLargest = arr[i];
            found = 1;
        }
    }
    if (found) {
        printf("%d", secondLargest);
    } else {
        printf("No second largest element");
    }
    return 0;
}
