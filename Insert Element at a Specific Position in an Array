#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N, position, value;
    scanf("%d", &N);
    int arr[N + 1];  
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d", &value);
    scanf("%d", &position);
    position -= 1;
    for (int i = N; i > position; i--) {
        arr[i] = arr[i - 1];
    }
    arr[position] = value;
    for (int i = 0; i <= N; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}
