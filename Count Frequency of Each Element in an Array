#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d", &N);
    int arr[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    for (int i = 0; i < N; i++) {
        int count = 0;
        int printed = 0;
        for (int k = 0; k < i; k++) {
            if (arr[i] == arr[k]) {
                printed = 1;
                break;
            }
        }
        if (!printed) {
            for (int j = 0; j < N; j++) {
                if (arr[i] == arr[j]) {
                    count++;
                }
            }
            printf("%d: %d\n", arr[i], count);
        }
    }
    return 0;
}
