#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N,count=0,x;
    scanf("%d",&N);
    int arr[N];
    for (int i=0;i<N;i++){
        scanf("%d",&arr[i]);
    }
    scanf("%d",&x);
    for (int i=0;i<N;i++){
        if(arr[i]==x){
            count++;
        }
    }
    printf("%d",count);
    return 0;
}
