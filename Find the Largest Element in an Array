#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N,max;
    scanf("%d",&N);
    int arr[N];
    for (int i=0;i<N;i++){
        scanf("%d",&arr[i]);
    }
    max=arr[0];
    for (int i=0;i<N;i++){
        if (arr[i]>max){
            max=arr[i];
        }
    }
    printf("%d",max);
    return 0;
}
