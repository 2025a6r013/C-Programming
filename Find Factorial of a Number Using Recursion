#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
long long factorial(int num) {
    if (num==0||num==1)
        return 1;
    return num*factorial(num-1);
}
int main() {
    int num;
    scanf("%d",&num);
    long long result=factorial(num);
    printf("%lld",result);
    return 0;
}
