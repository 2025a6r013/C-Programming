#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char arr[100];
    int len;
    char temp;
    scanf("%[^\n]",arr);
    len=strlen(arr);
    for (int i=0;i<len/2;i++){
        temp=arr[i];
        arr[i]=arr[len-i-1];
        arr[len-i-1]=temp;
    }
    printf("%s",arr);
    return 0;
}
