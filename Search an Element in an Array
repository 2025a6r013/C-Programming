#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N,x,found=0;
    scanf("%d", &N);
    int arr[N]; 
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d",&x);
    for (int i=0;i<N;i++){
        if (arr[i]==x){
            printf("Element found at position %d",i+1);
            found=1;
        }
    }
    if (found==0){
        printf("Element not found");
    }
    return 0;
}
