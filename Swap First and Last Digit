#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num, temp, count = 0, first, last, power;
    scanf("%d", &num);
    if (num >= 0 && num <= 9) {
        printf("%d", num);
    }
    else {
    temp = num;
    last = temp % 10;  
    for (; temp != 0; temp /= 10) {
        count++;
    }
    power = (int)pow(10, count - 1); 
    first = num / power;              
    int middle = (num % power) / 10;  
    int newNum = last * power + middle * 10 + first;
    printf("%d", newNum);
    }
    return 0;
}
