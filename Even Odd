#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n;
    scanf("%d",&n);
    if (n%2==0) {
        printf("Even\n");
    }
    else {
        printf("Odd\n");
    }
    return 0;
}


                                          OR
#include <stdio.h>
int main() {
    int i,n;
    printf("Enter the value of n: ");
    scanf("%d",&n);
    printf("Even numbers are: ",n);
    for (i=1;i<=n;i++){
        if (i%2==0){
            printf("%d ",i);
        }
    }
    printf("\nOdd numbers are: ",n);
    for (i=1;i<=n;i++){
        if (i%2!=0){
            printf("%d ",i);
        }
    }
    return 0;
}
