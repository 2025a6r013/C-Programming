#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int reverse(int num, int rev){
    if (num==0){
        return rev;
    }
    return reverse(num/10,rev*10+num%10);
}
int main() {
    int num;
    scanf("%d",&num);
    int result=reverse(num,0);
    printf("%d",result);    
    return 0;
}
