#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <math.h>
int isArmstrong(int n) {
    int original = n, sum = 0, digits = 0;
    for (int temp = n; temp > 0; temp /= 10) {
        digits++;
    }
    for (int temp = n; temp > 0; temp /= 10) {
        int digit = temp % 10;
        sum += pow(digit, digits);
    }
    return sum == original;
}
int isPrime(int n) {
    if (n <= 1) return 0;  

    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) {
            return 0;  
        }
    }
    return 1;  
}
int isPerfect(int n) {
    int sum = 0;
    for (int i = 1; i <= n / 2; i++) {
        if (n % i == 0) {
            sum += i;  
        }
    }
    return sum == n;  
}
int main() {
    int num;
    scanf("%d", &num);
    if (isArmstrong(num)) {
        printf("Armstrong Number\n");
    } else if (isPrime(num)) {
        printf("Prime Number\n");
    } else if (isPerfect(num)) {
        printf("Perfect Number\n");
    } else {
        printf("None of these\n");
    }   
    return 0;
}
